    <% include common/_head %>
<body>
    <% include common/_header %>
    <div class="container">
        <div class="col-sm-8 mx-auto">
            <p>リプレイするpdfを選択</p>
            <div class="row">
                <% var array %>
                <div class="col">
                    <form method="post" action='/replaymenu'>
                        <input type='hidden' name='userName' value=<%= userName %>>
                        <% array.forEach(function(value,key){ %>
                            <div class="menuLayout">
                                <a href="/replay?id=<%= userName %>&pdfname=<%= value %>" type='submit'>
                                    <%= value %>
                                </a>
                                <!-- <input class="btn btn-light round-circle-0" type="submit" name='pdfname' value=<%= value %>> -->
                            </div>
                        <% }) ;%>
                    </form>
                </div>
            </div>
        </div>
        
    </div>
    
</body>
</html>